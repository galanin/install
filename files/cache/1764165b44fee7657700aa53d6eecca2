</dl>

<script type="text/javascript">
    $(document).ready(function() {
        $('#sForm').submit(function() {
            var mail = $('#emaiForSubscription').val();
            if (mail) {
                var mktoData = {};
                mktoData[MKTO_SOURCE_FIELD] = 'email-subscription';
                mktoDoAsync(mail, mktoData);
            }
            return false;
        });
    });
</script>

