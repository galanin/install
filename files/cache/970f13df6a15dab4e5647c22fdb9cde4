function pageSpecificOnReady() {
	if (navigator.platform.toUpperCase().indexOf('MAC') !== -1) {
		$('#pms-desktop .tabs').data('selectedTab', '[href="#tabs-2"]');
		$('#plex-pht .tabs').data('selectedTab', '[href="#tabs-16"]');
	} else if (navigator.platform.toUpperCase().indexOf('WIN') !== -1) {
		$('#pms-desktop .tabs').data('selectedTab', '[href="#tabs-1"]');
		$('#plex-pht .tabs').data('selectedTab', '[href="#tabs-15"]');
	} else if (navigator.platform.toUpperCase().indexOf('LINUX') !== -1) {
		$('#pms-desktop .tabs').data('selectedTab', '[href="#tabs-3"]');
	}
}
</script>

